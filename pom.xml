<?xml version="1.0" encoding="UTF-8"?>

<project
    xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>vavi</groupId>
  <artifactId>vavi-util-archive</artifactId>
  <name>Vavi Archiving API</name>
  <organization>
    <name></name>
  </organization>
  <version>1.0.0</version>
  <url>http://www.vavisoft.com/</url>
  <description></description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
          <encoding>Windows-31J</encoding>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <repositories>
  </repositories>
  <dependencies>
  	<dependency>
  		<groupId>vavi</groupId>
  		<artifactId>vavi-commons</artifactId>
  		<version>1.0.0</version>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>jlha</groupId>
  		<artifactId>jlha</artifactId>
  		<version>0.0.6</version>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>jbinhex</groupId>
  		<artifactId>jbinhex</artifactId>
  		<version>0.0.1</version>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>vavi</groupId>
  		<artifactId>vavi-commons-sandbox</artifactId>
  		<version>0.3.1</version>
  	</dependency>
  	<dependency>
  		<groupId>junit</groupId>
  		<artifactId>junit</artifactId>
  		<version>4.5</version>
  		<scope>test</scope>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>commons-compress</groupId>
  		<artifactId>commons-compress</artifactId>
  		<version>1.0</version>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  	  <groupId>de.innosystec</groupId>
  	  <artifactId>java-unrar</artifactId>
  	  <version>1.7.0-SNAPSHOT</version>
  	</dependency>
  </dependencies>
</project>